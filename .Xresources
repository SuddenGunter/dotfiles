URxvt.perl-ext-common: default,font-size,matcher

URxvt*termName: rxvt-unicode-256color

URxvt.font: xft:Hack-Regular:pixelsize=16

URxvt*scrollBar: false
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C

! Panda for Xresources terminals
! Support: https://github.com/SuddenGunter
! Based on https://github.com/PandaTheme/panda-terminal
! Converted via http://terminal.sexy

! special
*.foreground:   #f3f3f3
*.background:   #1f1f20
*.cursorColor:  #f3f3f3

! black
*.color0:       #2a2d32
*.color8:       #2a2d32

! red
*.color1:       #ff276d
*.color9:       #ff276d

! green
*.color2:       #00ffdb
*.color10:      #00ffdb

! yellow
*.color3:       #ffb863
*.color11:      #ffb863

! blue
*.color4:       #6cb1ff
*.color12:      #6cb1ff

! magenta
*.color5:       #b184ef
*.color13:      #b184ef

! cyan
*.color6:       #67d3c2
*.color14:      #67d3c2

! white
*.color7:       #f3f3f3
*.color15:      #f3f3f0
